# test sequence and analysis

Document d
    classCode = /^S/

Segment@d s

sequence@s seq
    Token t1
        xpos2 = "ART"
    Token t2
        lemma = "true"
        xpos2 = "ADJ"
    Token t3
        xpos2 = "SUBST"

kwic => plain
    context
        s
    entities
        t1
        t2
        t3

nounDist => analysis
    attributes
        t3.lemma
    functions
        frequency
    filter
        frequency > 1
