SELECT
    s.Segment_id AS seg_id, Document.document_id AS Document_id, Document."meta"::jsonb AS Document_meta, Document."char_range" AS Document_char_range, s.segment_id AS Segment_id, s."char_range" AS Segment_char_range
FROM
    preps
    LEFT JOIN bnc1.document Document ON Document.char_range @> s.char_range
GROUP BY
    Document.document_id, Document."char_range", s.segment_id, s."char_range"